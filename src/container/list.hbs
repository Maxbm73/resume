<div>
    <ul class="mt-2">
        {{#each list}}
        <li class="mb-2">

            <button {{#if @last}}disabled{{/if}}>{{name}}-{{@index}}</button>

            <span>{{name}}</span>
        </li>
        {{/each}}
    </ul>

    <ul>
        {{#each user}}
        <li>
            {{this}}
        </li>
        {{/each}}
    </ul>
</div>